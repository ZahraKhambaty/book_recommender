    "count_no_age = users.loc[users.Age == 'NULL','Age'].count()\n",
    "Age_not_null = users.loc[(users.Age != 'NULL') & (users.Age != '')]\n",
    "Age_not_null[['Age']] = Age_not_null[['Age']].astype(int)\n",
    "# Age distribution\n",
    "plt.rc(\"font\", size=16)\n",
